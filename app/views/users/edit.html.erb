<h1>Sign Up</h1>

<%= form_with(model:@user,local:true, url: user_path) do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :email %>
<%= f.email_field :email %>


  
<%= f.label :image %>
<%= image_tag(@user.image.url) if @user.image && @user.image.url %>
<%= f.file_field :image %>
<%= f.hidden_field :image_cache %>
  

<%= f.submit "Create my account" %>

<% end %>